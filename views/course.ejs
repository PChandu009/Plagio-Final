<div id="viewAssignments">
        <div class="col-md-12" id="editCourseForm">
        <div class="jumbotron">
            <div class="editCourseBlock">
                <h4>
                    <i class="fa fa-eye" aria-hidden="true"></i> View Assignments</h4><hr/>
                <div class="dash-line"></div>
                <table class="table table-hover assignmentsList">
            <tbody>
                <% for(let i=0; i<assignments.length; i++ ) { %>
                    <tr>
                        <td>
                            <a href="/files/<%= assignments[i]._id %>"><i class="glyphicon glyphicon-folder-close"></i> <%= assignments[i].name %></a>
                        </td>
                    </tr>
                    <% } %>
                </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
